{
    "compilerOptions": {
        "target": "es5",
        "lib": [
            "dom",
            "dom.iterable",
            "esnext"
        ],
        "allowJs": true,
        "skipLibCheck": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "noEmit": true,
        "noImplicitAny": false,
        "esModuleInterop": true,
        "module": "esnext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "jsx": "preserve",
        "incremental": true,
        "baseUrl": "./app/",
        "paths": {
            "@/components": [
                "components/*"
            ],
            "@/assets/*": [
                "assets/*"
            ],
            "@/hooks/*": [
                "hooks/*"
            ],
            "@/services/*": [
                "services"
            ],
            "@types/*": [
                "types/*"
            ],
            "@/store/*": [
                "store/*"
            ],
            "@/utils/*": [
                "utils/*"
            ]
        }
    },
    "include": [
        "next-env.d.ts",
        "**/*.ts",
        "**/*.tsx",
        "src/pages/component/user/Mheader.js",
        "app/component/layout/headrer/search"
    ],
    "exclude": [
        "node_modules"
    ]
}